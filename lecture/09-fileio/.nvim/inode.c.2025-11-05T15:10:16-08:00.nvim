#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <sys/stat.h>

int main() {
  char *buffer;
  ssize_t len = getline(&buffer, NULL, stdin);

  struct stat sb;

  int status = stat(buffer, &sb);

  if(S_ISREG(status)) printf("Regular file");
  else if (S_ISDIR(status)) printf("Directory");
  else printf("Other");
}
