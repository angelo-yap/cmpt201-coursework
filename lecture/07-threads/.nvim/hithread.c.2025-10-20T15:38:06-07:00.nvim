#define _GNU_SOURCE
#include <pthread.h>
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

void *printHello(void *word) {
  printf("%s, TD:%d\n", (char *)word, gettid());
  return 0;
}

int main() {

  pthread_t thread; //= malloc(sizeof(pthread_t));

  int status =
      pthread_create(&thread, NULL, &printHello, "john bigdick");

  pthread_join(thread, NULL);

  return 0;
}
